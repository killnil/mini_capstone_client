<% @products.each do |product| %>
  <h1><a href="/client/products/<%= product['id'] %>"><%= product['name'] %></a></h1>
  <h4><%= product['formatted']['price'] %></h4>
  <p><%= product['description'] %></p>
  <% if product['images'] != [] %>
    <img src="<%= product['images'][0]['url'] %>" alt="">
  <% end %>
  <br>
<% end %>